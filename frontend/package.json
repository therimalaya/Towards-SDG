{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "cd research && yarn build && cd .. && cd education && yarn build && cd ..",
    "prepare": "rm -rf ./home/research && rm -rf ./home/education",
    "deploy": "mv ./research/build ./home/research && mv ./education/build ./home/education",
    "start": "yarn run build && yarn run prepare && yarn run deploy && nodemon --exec babel-node index.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "nodemon": "^2.0.3"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
